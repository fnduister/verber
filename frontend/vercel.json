{
    "name": "verber-frontend",
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://your-backend-url.railway.app/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "REACT_APP_API_URL": "https://your-backend-url.railway.app/api",
        "REACT_APP_WS_URL": "wss://your-backend-url.railway.app"
    }
}